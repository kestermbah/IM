{
    "version": "0.2.0",
    "configurations": [
      {
        "name": ".NET MAUI Debug (Android)",
        "type": "coreclr",
        "request": "launch",
        "preLaunchTask": "build",
        "program": "${workspaceFolder}/bin/Debug/net8.0-android/InventoryMaui.dll",
        "args": [],
        "cwd": "${workspaceFolder}",
        "stopAtEntry": false,
        "console": "internalConsole",
        "internalConsoleOptions": "openOnSessionStart",
        "launchBrowser": {
          "enabled": false,
          "args": "${auto-detect-url}"
        },
        "env": {
          "ASPNETCORE_ENVIRONMENT": "Development"
        },
        "sourceFileMap": {
          "/Views": "${workspaceFolder}/Views"
        }
      },
      {
        "name": ".NET MAUI Debug (iOS)",
